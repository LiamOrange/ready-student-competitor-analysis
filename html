        // Tab navigation
        document.querySelectorAll('.nav-tab').forEach(tab => {
            tab.addEventListener('click', () => {
                document.querySelectorAll('.nav-tab').forEach(t => t.classList.remove('active'));
                document.querySelectorAll('.tab-content').forEach(c => c.classList.remove('active'));
                
                tab.classList.add('active');
                const tabId = tab.getAttribute('data-tab');
                document.getElementById(tabId).classList.add('active');
            });
        });

        // Populate competitor cards
        function populateCompetitors() {
            const grid = document.getElementById('competitors-grid');
            grid.innerHTML = competitors.map(comp => `
                <div class="bg-white rounded-lg shadow p-6">
                    <div class="flex justify-between items-start mb-4">
                        <h3 class="text-xl font-semibold text-gray-900">${comp.name}</h3>
                        <div class="flex space-x-2">
                            <button class="text-gray-400 hover:text-blue-600">✏️</button>
                            <button class="text-gray-400 hover:text-red-600">🗑️</button>
                        </div>
                    </div>
                    
                    <p class="text-gray-600 mb-4">${comp.description}</p>
                    
                    <div class="flex items-center mb-4">
                        <span class="text-sm text-gray-500">Market Share:</span>
                        <span class="ml-2 text-sm font-medium text-gray-900">${comp.marketShare}%</span>
                    </div>

                    <div class="mb-4">
                        <span class="text-sm text-gray-500">AI/ML Status:</span>
                        <span class="ml-2 inline-flex items-center px-2 py-1 rounded-full text-xs font-medium ${
                            comp.aiMlFeatures.current.length > 0 ? 'bg-green-100 text-green-800' : 
                            comp.aiMlFeatures.planned.length > 0 ? 'bg-yellow-100 text-yellow-800' : 
                            'bg-red-100 text-red-800'
                        }">
                            ${comp.aiMlFeatures.current.length > 0 ? 'Advanced' : 
                              comp.aiMlFeatures.planned.length > 0 ? 'Planned' : 'None'}
                        </span>
                    </div>
                    
                    <div class="flex items-center justify-between">
                        <a href="${comp.website}" target="_blank" rel="noopener noreferrer" class="text-blue-600 hover:text-blue-800 flex items-center text-sm">
                            🔗 Visit Website
                        </a>
                        <span class="text-xs text-gray-400">
                            Updated: ${new Date().toLocaleDateString()}
                        </span>
                    </div>
                </div>
            `).join('');
        }

        // Populate SWOT analysis
        function populateSWOT() {
            const content = document.getElementById('swot-content');
            
            // Create competitor selector
            const selector = `
                <div class="mb-8">
                    <label class="block text-sm font-medium text-gray-700 mb-2">
                        Select Competitor
                    </label>
                    <select id="swot-competitor-select" class="block w-full max-w-xs px-3 py-2 border border-gray-300 rounded-md shadow-sm focus:outline-none focus:ring-blue-500 focus:border-blue-500">
                        ${competitors.map(comp => `<option value="${comp.id}">${comp.name}</option>`).join('')}
                    </select>
                </div>
                <div id="swot-grid" class="grid grid-cols-1 md:grid-cols-2 gap-6">
                </div>
            `;
            
            content.innerHTML = selector;
            
            // Show SWOT for first competitor
            showSWOT(competitors[0]);
            
            // Add change event listener
            document.getElementById('swot-competitor-select').addEventListener('change', (e) => {
                const competitor = competitors.find(c => c.id === e.target.value);
                if (competitor) showSWOT(competitor);
            });
        }
        
        function showSWOT(competitor) {
            const grid = document.getElementById('swot-grid');
            
            // Generate SWOT data based on CAUDIT capabilities and market position
            const strengths = [
                `Strong ${competitor.cauditCapabilities.studentLifecycle.toLowerCase()} student lifecycle management`,
                `${competitor.cauditCapabilities.reportingAnalytics} reporting and analytics capabilities`,
                `${competitor.marketShare}% market share in SMS market`,
                competitor.aiMlFeatures.current.length > 0 ? 'Advanced AI/ML capabilities' : 'Established market presence'
            ];
            
            const weaknesses = [
                competitor.cauditCapabilities.aiMl === 'None' ? 'Limited AI/ML capabilities' : 'High implementation complexity',
                competitor.cauditCapabilities.integrationAPIs === 'Basic' ? 'Limited integration capabilities' : 'Legacy architecture constraints',
                'High cost of ownership',
                'Complex implementation process'
            ];
            
            const opportunities = [
                'Growing demand for cloud-native solutions',
                'AI/ML market expansion opportunities',
                'International market expansion',
                'SaaS transformation opportunities'
            ];
            
            const threats = [
                'New cloud-native competitors',
                'Changing education technology landscape',
                'Budget constraints in education sector',
                'Technology disruption from AI/ML'
            ];
            
            const swotCategories = [
                { title: 'Strengths', items: strengths, color: 'bg-green-50 border-green-200' },
                { title: 'Weaknesses', items: weaknesses, color: 'bg-red-50 border-red-200' },
                { title: 'Opportunities', items: opportunities, color: 'bg-blue-50 border-blue-200' },
                { title: 'Threats', items: threats, color: 'bg-yellow-50 border-yellow-200' }
            ];
            
            grid.innerHTML = swotCategories.map(category => `
                <div class="p-6 rounded-lg border ${category.color}">
                    <h3 class="text-lg font-semibold mb-4">${category.title}</h3>
                    <ul class="space-y-2">
                        ${category.items.map(item => `
                            <li class="flex items-start">
                                <span class="inline-block w-2 h-2 bg-gray-400 rounded-full mt-2 mr-3 flex-shrink-0"></span>
                                <span class="text-sm text-gray-700">${item}</span>
                            </li>
                        `).join('')}
                    </ul>
                </div>
            `).join('');
        }

        // Populate RTO Analysis
        function populateRTOAnalysis() {
            const rtoCompetitors = competitors.filter(comp => 
                comp.name === 'aXcelerate' || comp.name === 'TechnologyOne'
            );
            
            const rtoContent = document.querySelector('#rto .max-w-7xl');
            rtoContent.innerHTML = `
                <h1 class="text-3xl font-bold text-gray-900 mb-8">RTO/VET Market Analysis</h1>
                
                <div class="mb-8">
                    <p class="text-gray-600">Specialized analysis of Australian RTO management system competitors with enhanced CAUDIT capabilities assessment.</p>
                </div>

                <!-- RTO Market Overview -->
                <div class="grid grid-cols-1 md:grid-cols-4 gap-6 mb-8">
